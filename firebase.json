{
  "hosting": {
    "public": "./",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "package.json",
      "README.markdown",
      "**/presentation/**",
      "**/assets/**",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}